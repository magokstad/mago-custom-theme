      // Base / unsupported colors
	  "tokenColors": [
		{
		  "scope": [
			"variable",
			"string constant.other.placeholder",
			"invalid.deprecated",
			"keyword",
			"storage.type",
			"storage.modifier"
		  ],
		  "settings": {
			"foreground": "#A0A0A0"
		  }
		},
        // What does this do?
		{
		  "scope": [
			"invalid",
			"invalid.illegal"
		  ],
		  "settings": {
			"foreground": "#FF293B"
		  }
		},
		// Hmmmmmm what is this one...
		{
		  "scope": [
			"constant.character.escape"
		  ],
		  "settings": {
			"foreground": "#ABABAB"
		  }
		},
		{
		  "scope": [
			"*url*",
			"*link*",
			"*uri*"
		  ],
		  "settings": {
			"fontStyle": "underline"
		  }
		},
